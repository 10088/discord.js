{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["**/dist/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["**/coverage/**"]
		},
		"lint": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"format": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"docs": {
			"dependsOn": ["^build"],
			"outputs": ["**/docs/docs.json", "**/docs/docs.api.json"]
		},
		"changelog": {
			"dependsOn": ["^build"],
			"outputs": ["CHANGELOG.md"]
		}
	}
}
